<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
    
     <bean id="employee" class="com.cg.springassignment1.usingxmlannotation.Employee">
       <property name="empId" value="100"/>
       <property name="empName" value="Rama"/>
       <property name="salary" value="12345.67"/>
      </bean>
       <bean id="client" class="com.cg.springassignment1.usingxmlannotation.Client" autowire="byType">
        <property name="emp">
      
       <list>
       <ref bean="employee"/>
       </list>
      
      </property>
       
     </bean>
           
  
 </beans>        